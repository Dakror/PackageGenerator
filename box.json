{
    "alias": "wsdltophp.phar",
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "phpunit",
                "Tests",
                "tests"
            ],
            "in": "vendor"
        },
        {
            "name": "*.json",
            "exclude": [
                "phpunit",
                "Tests",
                "tests"
            ],
            "in": "vendor/composer/composer"
        },
        {
            "name": "LICENSE",
            "exclude": [
                "phpunit",
                "Tests",
                "tests"
            ],
            "in": "vendor/composer/composer"
        },
        {
            "name": "cacert.pem",
            "exclude": [
                "phpunit",
                "Tests",
                "tests"
            ],
            "in": "vendor/composer/ca-bundle/res"
        }
    ],
    "files": [
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "git-version": "git-version",
    "main": "console",
    "output": "bin/wsdltophp.phar",
    "stub": true
}
